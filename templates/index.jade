extends layout

block title
  title Ifi-ordenen

block content
  h1= page.title
  != page.html
  ul.classes
    each _class in locals.structure
      li
        h3= _class.name
        if _class.members.length > 0
          ul
            each username in _class.members
              - var person = contents.person[username]['index.md'].metadata
              li
                a(href='person/' + username)
                  img(src='person/' + username + '/' + username + '.jpg', title=)
        else
          p Ingen medlemmer enda