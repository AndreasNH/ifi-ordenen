ul.classes
  each _class in structureFile.structure
    li
      h3= _class.name
      if _class.members.length > 0
        ul.overview
          each username in _class.members
            - var person = individualPath[username]['index.md'].metadata
            li
              a(href=locals.url+ structureFile.path + username)
                img.profile(src=locals.url + structureFile.path + username + '/thumb.jpg', title=person.title)
      else
        p= structureFile.noMembersExplanation